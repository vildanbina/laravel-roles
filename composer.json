{
    "name": "vildanbina/laravel-roles",
    "description": "Powerful package for handling roles, permissions, and levels in Laravel. Supports Laravel 5.3 and higher",
    "keywords": [
        "laravel-roles",
        "laravel-permissions",
        "laravel-levels",
        "roles",
        "permissions",
        "acl",
        "levels",
        "laravel"
    ],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Vildan Bina",
            "email": "vildanbina@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "eklundkristoffer/seedster": "^3.0",
        "laravel/helpers": "^1.2"
    },
    "require-dev": {
        "illuminate/support": "^5.8",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "^9.0", 
    },
    "autoload": {
        "psr-4": {
            "vildanbina\\LaravelRoles\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "vildanbina\\LaravelRoles\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "vildanbina\\LaravelRoles\\RolesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
